{ 
  "manifest_version" : 2,
  "name" : "Chrome-Extension: replace Image",
  "version" : "0.1",
  "web_accessible_resources" : [
  "images/*.jpeg","images/*.png"
  ],"content_scripts" : [
    {
    "matches" : [
    "<all_urls>"
    ],
    "js" : ["content.js"]
    }
    ],
  "browser_action" : {
  "default_icon" : "r.png",
  "default_popup" : "popup.html"
  }
  }